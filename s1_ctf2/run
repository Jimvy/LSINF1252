#! /bin/bash

g++ main.cpp
last=$(getinput @username | sha256sum)

f=$(echo oboroxx | sha256sum)
last=$(./a.out $f $last)

f=$(echo systeminfocestgenial | sha256sum)
last=$(./a.out $f $last)

f=$(echo batman | sha256sum)
last=$(./a.out $f $last)

f=$(echo bisounours | sha256sum)
last=$(./a.out $f $last)

f=$(echo foobar | sha256sum)
last=$(./a.out $f $last)

rep=$(getinput q1)
if [ "$rep" == "$last" ]
then
	feedback-result success
else
	feedback-result failed
	feedback-msg -m "Ce n'est pas le bon code, avez-vous entré votre pseudo inginious et pas ucl ? Ce n'est pas d'office le même."
fi
